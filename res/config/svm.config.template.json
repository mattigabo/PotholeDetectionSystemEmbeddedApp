{
  "svm": {
    "train-set": "/path/to/train/set/{file}",
    "test-set": "/path/to/test/set/{file}",
    "model": "/path/to/svm/model.yml",
    "norm-method": "MIN-MAX | L1 | L2 | HAMMING | HAMMING2",
